{
    "name": "aplicacionjuridica-web",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite --port 3000",
        "dev:simple": "vite serve index-simple.html --port 3000",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx src",
        "lint:fix": "npm run lint -- --fix",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md}\"",
        "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md}\"",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "coverage": "vitest --coverage",
        "test:e2e": "playwright test",
        "test:e2e:auth": "playwright test auth.spec.ts",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:debug": "playwright test --debug"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.9",
        "@heroicons/react": "^2.2.0",
        "@vitejs/plugin-react": "^5.0.4",
        "axios": "^1.7.7",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "framer-motion": "^12.23.24",
        "lucide-react": "^0.544.0",
        "react": "18.3.1",
        "react-big-calendar": "^1.19.4",
        "react-dom": "18.3.1",
        "react-dropzone": "^14.3.8",
        "react-intersection-observer": "^9.16.0",
        "react-markdown": "^10.1.0",
        "react-router-dom": "6.26.2",
        "react-type-animation": "^3.2.0",
        "recharts": "^3.3.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "^24.7.0",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.1",
        "@typescript-eslint/eslint-plugin": "^8.12.2",
        "@typescript-eslint/parser": "^8.12.2",
        "@vitest/ui": "^3.0.0",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.9.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "jsdom": "^25.0.1",
        "postcss": "^8.5.6",
        "prettier": "^3.3.3",
        "tailwindcss": "^3.4.18",
        "terser": "^5.44.0",
        "typescript": "^5.9.3",
        "vite": "^5.0.0",
        "vitest": "^3.0.0"
    }
